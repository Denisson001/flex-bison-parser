Grammar

    0 $accept: PROGRAM $end

    1 PROGRAM: UNIT

    2 UNIT: '{' OPERATIONS '}'
    3     | '{' '}'

    4 OPERATIONS: OPERATION
    5           | OPERATIONS OPERATION

    6 OPERATION: PRINT
    7          | READ
    8          | ASSIGN
    9          | IF
   10          | WHILE

   11 ASSIGN: NUMBER_VAR '=' NUMBER_EXPR ';'
   12       | STRING_VAR '=' STRING_EXPR ';'
   13       | STRING_VAR '[' NUMBER_EXPR ']' '=' STRING_EXPR ';'

   14 PRINT: _PRINT '(' NUMBER_EXPR ')' ';'
   15      | _PRINT '(' STRING_EXPR ')' ';'

   16 READ: _READ '(' NUMBER_VAR ')' ';'
   17     | _READ '(' STRING_VAR ')' ';'

   18 IF: _IF '(' BOOL_EXPR ')' UNIT
   19   | _IF '(' BOOL_EXPR ')' UNIT _ELSE UNIT

   20 WHILE: _WHILE '(' BOOL_EXPR ')' UNIT

   21 STRING_VAR: _STRING_VAR

   22 STRING_EXPR: CONCAT_EXPR
   23            | STRING_EXPR '+' CONCAT_EXPR

   24 CONCAT_EXPR: '(' STRING_EXPR ')'
   25            | STRING_TERM
   26            | SLICE_EXPR

   27 SLICE_EXPR: CONCAT_EXPR '[' NUMBER_EXPR ':' NUMBER_EXPR ']'
   28           | CONCAT_EXPR '[' NUMBER_EXPR ']'

   29 STRING_TERM: STRING_VAR
   30            | _STRING

   31 NUMBER_VAR: _NUMBER_VAR

   32 NUMBER_EXPR: NUMBER_EXPR '+' ADD_EXPR
   33            | NUMBER_EXPR '-' ADD_EXPR
   34            | ADD_EXPR

   35 ADD_EXPR: ADD_EXPR '*' MULT_EXPR
   36         | ADD_EXPR '/' MULT_EXPR
   37         | MULT_EXPR

   38 MULT_EXPR: MULT_EXPR '%' MOD_EXPR
   39          | MOD_EXPR

   40 MOD_EXPR: '(' NUMBER_EXPR ')'
   41         | NUMBER_TERM

   42 NUMBER_TERM: NUMBER_VAR
   43            | _NUMBER
   44            | _LEN_FUNC '(' STRING_EXPR ')'

   45 BOOL_EXPR: BOOL_EXPR '|' '|' OR_EXPR
   46          | OR_EXPR

   47 OR_EXPR: OR_EXPR '&' '&' AND_EXPR
   48        | AND_EXPR

   49 AND_EXPR: BOOL_TERM
   50         | '(' BOOL_EXPR ')'
   51         | '!' '(' BOOL_EXPR ')'

   52 BOOL_TERM: NUMBER_EXPR BOOL_FUNCTION NUMBER_EXPR
   53          | STRING_EXPR BOOL_FUNCTION STRING_EXPR

   54 BOOL_FUNCTION: '=' '='
   55              | '!' '='
   56              | '<' '='
   57              | '>' '='
   58              | '<'
   59              | '>'


